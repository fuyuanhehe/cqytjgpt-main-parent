<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

  <!-- 数据库驱动包位置 -->  
   <classPathEntry  
       location="E:\maven\repository\mysql\mysql-connector-java\5.1.46\mysql-connector-java-5.1.46.jar" />

  
  <context id="context1" defaultModelType="flat" targetRuntime="MyBatis3">
    <!-- 指定生成的java文件的编码,没有直接生成到项目时中文可能会乱码 -->
    <property name="javaFileEncoding" value="UTF-8"/>
        <!-- 格式化java代码 -->
    <property name="javaFormatter" value="org.mybatis.generator.api.dom.DefaultJavaFormatter"/>
    <!-- 格式化XML代码 -->
    <property name="xmlFormatter" value="org.mybatis.generator.api.dom.DefaultXmlFormatter"/>
    
    <!-- 自动识别数据库关键字，默认false，如果设置为true，根据SqlReservedWords中定义的关键字列表；
           一般保留默认值，遇到数据库关键字（Java关键字），使用columnOverride(反单引号 ；例如：`status`)覆盖
     -->
    <property name="autoDelimitKeywords" value="false"/>
    
    <!-- beginningDelimiter和endingDelimiter：指明数据库的用于标记数据库对象名的符号，比如ORACLE就是双引号，MYSQL默认是`反引号； -->
    <property name="beginningDelimiter" value="`"/>
    <property name="endingDelimiter" value="`"/>
    

    <commentGenerator type="com.ccttic.mybatis.generator.MyCommentGenerator">
          <!-- 是否去除自动生成的注释 true：是 ： false:否 -->  
          <property name="suppressAllComments" value="false"/>
          <!-- 阻止**生成的注释包含时间戳 -->
          <property name="suppressDate" value="true"/>
    </commentGenerator> 
  
   <jdbcConnection connectionURL="jdbc:mysql://10.10.20.22:3306/cqytjgpt" driverClass="com.mysql.jdbc.Driver" password="ccttic!" userId="root"/>

    
    <!-- 生成模型的包名和位置 -->  
    <javaModelGenerator targetPackage="com.ccttic.entity" targetProject="cqytjgpt-web-api/src/test/java" >
      <!--  for MyBatis3/MyBatis3Simple
            自动为每一个生成的类创建一个构造方法，构造方法包含了所有的field；而不是使用setter；
         -->
        <property name="constructorBased" value="false"/> 
         <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        <!--实体父类 <property name="rootClass" value="com.rls.xt.domain.entity.BaseEntity"/> -->
    </javaModelGenerator>
    
    <!-- 生成的映射文件报名和位置 -->  
    <sqlMapGenerator targetPackage="com.ccttic.entity" targetProject="cqytjgpt-web-api/src/test/java" />
    <!-- 生成DAO的包名和位置 -->
    <javaClientGenerator targetPackage="com.ccttic.mapper" targetProject="cqytjgpt-web-api/src/test/java" type="XMLMAPPER" />
    <!-- 要生成的那些表(更改tableName 和domainObjectName 就可以了) -->
    

<!--     <table tableName="X_EXECUTOR_LIST">
        <property name="useActualColumnNames" value="true"/>
    </table> -->
    

